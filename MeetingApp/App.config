<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>

	<connectionStrings>
		<!-- Windows Authentication -->
		<add name="MeetingDB" connectionString="Server=YOUR_SERVER_NAME;Database=MeetingAppDb;Trusted_Connection=True;" providerName="System.Data.SqlClient" />

		<!-- SQL Server Authentication -->
		<add name="MyDbConnectionString" connectionString="Server=YOUR_SERVER_NAME;Database=YOUR_DATABASE_NAME;User Id=YOUR_USER_ID;Password=YOUR_PASSWORD;" providerName="System.Data.SqlClient" />

		<!-- Bağlantı Zaman Aşımı (Connection Timeout) -->
		<add name="TimeoutConnection" connectionString="Server=YOUR_SERVER_NAME;Database=YOUR_DATABASE_NAME;User Id=YOUR_USER_ID;Password=YOUR_PASSWORD;Connection Timeout=30;" providerName="System.Data.SqlClient" />

		<!-- Belirli Bir Port Üzerinden Bağlantı -->
		<add name="PortConnection" connectionString="Server=YOUR_SERVER_NAME,PORT_NUMBER;Database=YOUR_DATABASE_NAME;User Id=YOUR_USER_ID;Password=YOUR_PASSWORD;" providerName="System.Data.SqlClient" />

		<!-- Adlandırılmış Örnek Üzerinden Bağlantı -->
		<add name="NamedInstanceConnection" connectionString="Server=YOUR_SERVER_NAME\\INSTANCE_NAME;Database=YOUR_DATABASE_NAME;User Id=YOUR_USER_ID;Password=YOUR_PASSWORD;" providerName="System.Data.SqlClient" />

		<!-- SQL Server Express LocalDB -->
		<add name="LocalDBConnection" connectionString="Server=(localdb)\\MSSQLLocalDB;Database=YOUR_DATABASE_NAME;Trusted_Connection=True;" providerName="System.Data.SqlClient" />

		<!-- MDF Dosyası ile Bağlantı (AttachDbFilename) -->
		<add name="AttachDbConnection" connectionString="Server=YOUR_SERVER_NAME;Database=YOUR_DATABASE_NAME;User Id=YOUR_USER_ID;Password=YOUR_PASSWORD;AttachDbFilename=C:\\Path\\To\\YourDatabaseFile.mdf;Integrated Security=True;" providerName="System.Data.SqlClient" />

		<!-- MARS (Multiple Active Result Sets) Desteği ile Bağlantı -->
		<add name="MarsConnection" connectionString="Server=YOUR_SERVER_NAME;Database=YOUR_DATABASE_NAME;User Id=YOUR_USER_ID;Password=YOUR_PASSWORD;MultipleActiveResultSets=True;" providerName="System.Data.SqlClient" />

		<!-- Şifreli Bağlantı (Encrypted Connection) -->
		<add name="EncryptedConnection" connectionString="Server=YOUR_SERVER_NAME;Database=YOUR_DATABASE_NAME;User Id=YOUR_USER_ID;Password=YOUR_PASSWORD;Encrypt=True;" providerName="System.Data.SqlClient" />

		<!-- Sunucu Sertifikasına Güvenme (TrustServerCertificate) -->
		<add name="TrustedServerConnection" connectionString="Server=YOUR_SERVER_NAME;Database=YOUR_DATABASE_NAME;User Id=YOUR_USER_ID;Password=YOUR_PASSWORD;Encrypt=True;TrustServerCertificate=True;" providerName="System.Data.SqlClient" />

		<!-- Yedek Sunucu ile Bağlantı (Failover Partner) -->
		<add name="FailoverPartnerConnection" connectionString="Server=PRIMARY_SERVER_NAME;Failover Partner=SECONDARY_SERVER_NAME;Database=YOUR_DATABASE_NAME;User Id=YOUR_USER_ID;Password=YOUR_PASSWORD;" providerName="System.Data.SqlClient" />

		<!-- Uygulama Adı ile Bağlantı (Application Name) -->
		<add name="AppNameConnection" connectionString="Server=YOUR_SERVER_NAME;Database=YOUR_DATABASE_NAME;User Id=YOUR_USER_ID;Password=YOUR_PASSWORD;Application Name=YourAppName;" providerName="System.Data.SqlClient" />

		<!-- Bağlantı Havuzu (Pooling) -->
		<add name="PoolingConnection" connectionString="Server=YOUR_SERVER_NAME;Database=YOUR_DATABASE_NAME;User Id=YOUR_USER_ID;Password=YOUR_PASSWORD;Pooling=True;Min Pool Size=0;Max Pool Size=100;Connection Lifetime=0;" providerName="System.Data.SqlClient" />
	</connectionStrings>
	<runtime>
